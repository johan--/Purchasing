:javascript
  $(function() {
    App.current_user.set('id', #{@me.id});
    App.current_user.set('username', "#{@me.username}");
    App.current_user.set('name', "#{@me.name}");
    App.current_user.set('email', "#{@me.email}");
    App.current_user.set('roles', #{@me.authorized_roles});
    App.current_user.set('photo_url', "#{@me.photo_url}");
  });
